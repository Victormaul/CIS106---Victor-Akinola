    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="Yes" type="String" array="False" size=""/>
            <declare name="answer" type="String" array="False" size=""/>
            <declare name="name" type="String" array="False" size=""/>
            <declare name="exam1" type="Real" array="False" size=""/>
            <declare name="exam2" type="Real" array="False" size=""/>
            <declare name="avg" type="Real" array="False" size=""/>
            <output expression="&quot;Would you like to run this program? Yes or No?&quot;" newline="True"/>
            <input variable="answer"/>
            <while expression="answer = &quot;Yes&quot;">
                <output expression="&quot;Enter your last name&quot;" newline="True"/>
