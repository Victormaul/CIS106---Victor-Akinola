                <input variable="exam1"/>
                <output expression="&quot;Enter exam 2 score&quot;" newline="True"/>
                <input variable="exam2"/>
                <assign variable="avg" expression="(exam1 + exam2)/2"/>
                <output expression="&quot;Your average: &quot; &amp; avg" newline="True"/>
            </while>
            <output expression="&quot;Program ended.&quot;" newline="True"/>
